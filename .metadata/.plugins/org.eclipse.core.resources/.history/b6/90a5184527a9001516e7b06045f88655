import java.util.ArrayList;

public class Lexer {
	
	static Input input = new Input("test.ink");
	private static ArrayList<Object> tokens = new ArrayList<Object>();
	
	public static void main (String[] args) {
		lex(input.readFile());
		System.out.println(input.readFile());
	}
	
	public static ArrayList<Object> lex (String input) {

		char[] toklist = input.toCharArray();
		ArrayList<Character> toks = new ArrayList<Character>();
		for (char tok : toklist) {
			if (tok != ' ') { //if token is not a space
				toks.add(tok);
			} else {
				
			}
		}

		return tokens;
	}
}
